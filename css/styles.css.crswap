    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
body {
    padding-top: 56px;
}
.img-fluid{
    max-height: 600px;
}
.form-control{
    border: none;
    border-bottom: 1px solid rgba(175, 175, 175, 0.75);
    font-size: 0.8em;
}
.form-control::placeholder{
    color: rgba(175, 175, 175, 0.75);
}
table{
    font-size: 0.7em;
}
